      <Label fx:id="name" prefHeight="26.0" prefWidth="204.0" styleClass="text-normal" text="Gà rán chiên giòn" />
      <Label fx:id="count" styleClass="text-header" text="SL: 500">
         <font>
            <Font name="Montserrat Black" size="12.0" />
         </font>
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
   </children>
   <stylesheets>
      <URL value="@../../../css/Admin.css" />
      <URL value="@../../../css/dashboardStyle.css" />
      <URL value="@../../../css/managementStyle.css" />
   </stylesheets>
   <padding>
      <Insets bottom="10.0" top="10.0" />
